{
  "algorithm": "rpl",
  "data_dir_train": "mri_cube_npy/",
  "data_dir_test": "test_data/",
  "model_checkpoint": "workspace/rpl_mri_data/weights-improvement-484.hdf5",
  "dataset_name": "mri_data",
  "train_data_generator_args": { "label_stem": "", "shuffle": true},
  "val_data_generator_args": {"label_stem": "", "shuffle": true },
  "test_data_generator_args": {"label_stem": "", "shuffle": true },

  "data_is_3D": true,
  "val_split": 0.05,

  "enc_filters": 16,
  "patches_per_side": 3,
  "data_dim": 128,

  "loss": "weighted_dice_loss",
  "scores": ["dice", "jaccard", "dice_pancreas_0", "dice_pancreas_1", "dice_pancreas_2"],
  "metrics": ["accuracy", "weighted_dice_coefficient", "weighted_dice_coefficient_per_class_pancreas"],

  "top_architecture": "big_fully",
  "prediction_architecture": "simple_binaryclass",
  "pooling": "max",
  "number_channels": 1,
  "batch_size": 2,

  "exp_splits": [50,25],
  "lr": 1e-3,
  "epochs_initialized": 400,
  "epochs_frozen": 0,
  "epochs_random": 0,
  "epochs_warmup": 25,
  "repetitions": 3,

  "clipnorm": 1,
  "clipvalue": 1,

  "num_classes": 2
}
